{
  "name": "foodfeed",
  "version": "1.0.0",
  "description": "A MERN app that connects viewers to livestream cooking channels",
  "main": "server.js",
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "connect-ensure-login": "^0.1.1",
    "connect-flash": "^0.1.1",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "mongoose": "^5.9.6",
    "node-media-server": "^2.1.6",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "react-router-dom": "^5.1.2",
    "react-scroll-to-bottom": "^1.3.2",
    "session-file-store": "^1.4.0",
    "shortid": "^2.2.15",
    "socket.io": "^2.3.0",
    "video.js": "^7.7.5"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adam-verma/FoodFeed.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adam-verma/FoodFeed/issues"
  },
  "homepage": "https://github.com/adam-verma/FoodFeed#readme"
}
